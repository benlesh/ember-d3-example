<svg width="{{width}}" height="{{height}}">
	<g class="x-axis">
		<line class="axis-line" x1="{{yAxisWidth}}" x2="{{yAxisWidth}}" y1="0" y2="{{graphHeight}}"></line>

		{{#each tick in xAxisTicks}}
			<text class="x-axis-tick" x="{{tick.x}}" y="{{tick.y}}">{{tick.value}}</text>
		{{/each}}
	</g>

	<g class="y-axis">
		<line class="axis-line" x1="{{yAxisWidth}}" x2="{{width}}" y1="{{graphHeight}}" y2="{{graphHeight}}"></line>	

		{{#each tick in yAxisTicks}}
			<text class="y-axis-tick" x="{{tick.x}}" y="{{tick.y}}">{{tick.value}}</text>
		{{/each}}	
	</g>	

	<path transform="{{lineTransform}}" class="line" d="{{d}}"></path>
</svg>

